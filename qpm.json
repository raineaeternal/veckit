{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.4.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "veckit",
    "id": "veckit",
    "version": "0.0.1",
    "url": null,
    "additionalData": {
      "overrideSoName": "libveckit.so",
      "cmake": false
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "cargo ndk --link-builtins -t arm64-v8a -o ./build build --release"
      ],
      "dev": ["cargo ndk --link-builtins -t arm64-v8a -o ./build/debug build"],
      "copy": ["pwsh ./scripts/copy.ps1"],
      "cpp": ["pwsh ./scripts/make_compile_db.ps1"],
      "ndk-stack": ["pwsh ./scripts/ndk-stack.ps1"]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": ["build/arm64-v8a"],
    "qmodIncludeFiles": [],
    "qmodOutput": "veckit.qmod"
  },
  "dependencies": [
    {
      "id": "bs-cordl",
      "versionRange": "^4200.0.0",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.7.0",
      "additionalData": {}
    }
  ]
}
